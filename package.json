  "module": "dist/index.js",
  "source": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurocode-io/cloudflare-workers-api.git"
  },
  "author": "neurocode I/O GmbH <info@neurocode.io>",
